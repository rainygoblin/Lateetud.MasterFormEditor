        function UploadFile(isAnyChangeInAnnotation) {
            try {
                var data = new FormData();
                var files = $("#file").get(0).files;
                if (files.length > 0) {
                    data.append("UploadedFile", files[0]);
                    $('#spfile').text("");
                    var ext = $('#file').val().split('.').pop().toLowerCase();
                    if ($.inArray(ext, ['gif', 'png', 'jpg', 'jpeg', 'tif', 'pdf']) == -1) {
                        Notify("Invalid file!", null, null, 'danger');
                        $("#file").val(null);
                        return;
                    }
                }
                else {
                    Notify("Please choose file", null, null, 'danger');
                    return;
                }
                $("#processText").html("Uploading page...");
                $("#loadingDialog").modal();
                var path = $('#BaseUrl').val() + '/api/MasterFormApi/UploadFile';
                $.ajax({
                    type: 'POST',
                    url: path,
                    contentType: false,
                    processData: false,
                    data: data,
                    success: function (data, textStatus, jqXHR) {
                        try {
                            if (textStatus == "success") {
                                var fileName = jqXHR["statusText"];
                                var fileKey = $('#hdnKey').val();
                                var friendlyName = $('#hdnfriendlyName').val();
                                CreateFiles(fileName, fileKey, friendlyName);
                            }
                        } catch (e) {
                            console.log("Error" + e.message);
                        }
                    }, error: function () {
                    }
                });
            }
            catch (e) {
                console.log("Error" + e.message);
            }
        }
        function CreateFiles(fileName, folderName, friendlyName) {
            var path = $('#BaseUrl').val() + '/api/GenerateFile/CreateFiles';
            $.ajax({
                type: 'POST',
                url: path,
                processData: false,
                data: 'FileName=' + fileName
                    + '&FolderName=' + folderName
                    + "&FriendlyName=" + friendlyName,
                success: function (data, textStatus, jqXHR) {
                    if (data && data.indexOf("successfully") > 0) {
                        Notify("Page added to master form set successfully", null, null, 'success');
                        window.location.reload();
                    }
                }
            });
        }
        //function back() {
        //    window.location.href = "../../Masterform/index";
        //}
    </script>

